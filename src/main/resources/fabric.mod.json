{
  "schemaVersion": 1,
  "id": "meteor-norbit",
  "version": "${version}",
  "name": "Meteor Norbit",
  "description": "Meteor Client addon to replace the event bus with norbit's implementation.",
  "authors": [
    "Crosby"
  ],
  "contact": {
    "repo": "https://github.com/RacoonDog/meteor-norbit",
    "sources": "https://github.com/RacoonDog/meteor-norbit",
    "issues": "https://github.com/RacoonDog/meteor-norbit/issues",
    "discord": "https://discord.gg/4RBmBCFSTc"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.racoondog.meteornorbit.MeteorNorbit"
    ]
  },
  "mixins": [
    "meteor-norbit.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "249,181,172"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
